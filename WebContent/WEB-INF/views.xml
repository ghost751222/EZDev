<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean name="jsonView" class="org.springframework.web.servlet.view.json.JsonView"/>

    <bean name="jsonPlainView" class="org.springframework.web.servlet.view.json.JsonView">
        <property name="contentType" value="text/plain"/>
    </bean>

    <bean name="jsonHTMLView" class="org.springframework.web.servlet.view.json.JsonView">
        <property name="contentType" value="text/html"/>
    </bean>

    <bean name="configView" class="com.consilium.view.ConfigView"/>



</beans>
